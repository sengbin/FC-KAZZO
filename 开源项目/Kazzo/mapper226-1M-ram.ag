function cpu_dump(d, pagesize, banksize)
{
	for(local i = 0; i < pagesize-1; i++){
		//cpu_write(d, 0x5000, 1);
		cpu_write(d, 0x8000, 1 <<5 );
		cpu_write(d, 0x5000, 0);
		cpu_write(d, 0x5001, i);
		cpu_read(d, 0x8000, 0x4000);
	}
	
	cpu_write(d, 0x5000, 2);
	cpu_write(d, 0x5001, 0xff);
	cpu_read(d, 0xc000, 0x4000);
}

